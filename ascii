#!/bin/bash

DIK="""
  ____ _____ _/  |_  _____      __| _/|__| ____ |  | __
 / __ \\\\__  \\\\   __\ \__  \    / __ | |  |/ ___\|  |/ /
\  ___/ / __ \|  |    / __ \_ / /_/ | |  \  \___|    < 
 \___  (____  /__|   (____  / \____ | |__|\___  >__|_ \\
     \/     \/            \/       \/         \/     \/
"""

GTFO="""
         __    _____       
   _____/  |__/ ____\____  
  / ___\   __\   __\/  _ \ 
 / /_/  >  |  |  | (  <_> )
 \___  /|__|  |__|  \____/ 
/_____/                    
                    .___              _____        __    __                   
  _____ _____     __| _/____  _____ _/ ____\____  |  | _|  | _______  _____   
 /     \\\\__  \   / __ |\__  \ \__  \\\\   __\\\\__  \ |  |/ /  |/ /\__  \ \__  \  
|  Y Y  \/ __ \_/ /_/ | / __ \_/ __ \|  |   / __ \|    <|    <  / __ \_/ __ \_
|__|_|  (____  /\____ |(____  (____  /__|  (____  /__|_ \__|_ \(____  (____  /
      \/     \/      \/     \/     \/           \/     \/    \/     \/     \/ 
"""

rando() {
    OPTS=(DIK GTFO)
    echo ${OPTS[$RANDOM%2]}
}

MSG=$(rando)
echo "${!MSG}"